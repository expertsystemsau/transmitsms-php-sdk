{
    "name": "expertsystemsau/transmitsms-client",
    "description": "Framework-agnostic PHP client for the TransmitSMS API",
    "keywords": [
        "ExpertSystems",
        "transmitsms",
        "sms",
        "api-client"
    ],
    "homepage": "https://github.com/expertsystemsau/transmitsms-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Mitchell Williams",
            "email": "mitchell@expertsystems.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "ExpertSystems\\TransmitSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExpertSystems\\TransmitSms\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
