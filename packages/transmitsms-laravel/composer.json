{
    "name": "expertsystemsau/transmitsms-laravel-client",
    "description": "Laravel notification channel and integration for the TransmitSMS API",
    "keywords": [
        "ExpertSystems",
        "laravel",
        "transmitsms",
        "sms",
        "notification-channel"
    ],
    "homepage": "https://github.com/expertsystemsau/transmitsms-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Mitchell Williams",
            "email": "mitchell@expertsystems.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "expertsystemsau/transmitsms-php-client": "^1.0",
        "illuminate/notifications": "^10.0||^11.0||^12.0",
        "illuminate/support": "^10.0||^11.0||^12.0",
        "saloonphp/laravel-plugin": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0||^10.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "ExpertSystems\\TransmitSms\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExpertSystems\\TransmitSms\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ExpertSystems\\TransmitSms\\Laravel\\TransmitSmsServiceProvider"
            ],
            "aliases": {
                "TransmitSms": "ExpertSystems\\TransmitSms\\Laravel\\Facades\\TransmitSms"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
